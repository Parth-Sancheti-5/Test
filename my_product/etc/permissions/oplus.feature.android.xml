<oplus-config>

    <!-- wenyao.li@BSP.SENSOR, 2022/09/20, add for QCOM Linear motor vibrator -->
    <oplus-feature name="oplus.software.vibrator_lmvibrator" />
    <oplus-feature name="oplus.hardware.vibrator_xlinear_type" />
    <oplus-feature name="oplus.software.vibrator_richctap" />
    <!-- wenyao.li@BSP.SENSOR, 2022/09/20, add end -->

    <!-- Shasha.Yu@ANDROID.SENSOR, 2022/11/18, Add for fusion light support-->
    <oplus-feature name="oplus.sensor.fusionlight.support" />
    <!-- Shasha.Yu@ANDROID.SENSOR, 2022/11/18, Add end-->

    <!-- Haifeng.Zhu@ANDROID.BIOMETIRC, 2021/05/13, add for feature face close eye verify -->
    <oplus-feature name="oplus.software.face_closeeye_detect"/>

    <!-- LuoBinqiang@ANDROID.AMS, 2020/07/14, add for some process keep alive -->
    <oplus-feature name="oplus.software.keep_alive" />
    <!-- LuoBinqiang@ANDROID.AMS, 2020/07/14, add end -->

    <!-- CaiChunchun@ANDROID.INPUT, 2020/11/25, add for wake up arouse -->
    <oplus-feature name="oplus.software.wake_up_arouse_support" />
    <!-- CaiChunchun@ANDROID.INPUT, 2020/11/25, add end -->
    <!-- CaiChunchun@ANDROID.INPUT, 2021/07/07, add for blackgesture -->
    <oplus-feature name="oplus.software.gesture_proximitysensor" />
    <!-- CaiChunchun@ANDROID.INPUT, 2021/07/07, add end -->
    <!--  2022/10/08, add for otg only in SM8475 -->
    <oplus-feature name="oplus.typecotg.connection.menu.support" />
    <!-- steve.shen@OPTI, 2021/03/12, Add for animation scroll opt -->
    <oplus-feature name="oplus.software.scroll.animation_opt" />
	<!-- Cheng.Li@ANDROID.Memory, 2022/10/24, Disable delayed kill cached processes to reduce the impact on boot memory. -->
    <oplus-feature name="oplus.software.ams.disable_delay_mcp_kill" />
	<!-- LiCheng@ANDROID.WMS, 2022/10/24,add for app resolution -->
    <oplus-feature name="oplus.software.app_resolution_switch" />
    <oplus-feature name="oplus.software.app_resolution_auto" />

    <!-- GaoWanpeng@ANDROID.AMS, 5214278, 2023/07/26, Add for keep alive oplus customization important apps -->
    <oplus-feature name="oplus.software.perceptible_app_keep_alive" />
    <oplus-feature name="oplus.software.bind_small_core" />

    <!-- shangxiaopeng@ANDROID.WMS, 2023/05/30, add for dynamic report rate -->
    <oplus-feature name="oplus.software.dynamic_report_rate" />
    <!-- shangxiaopeng@ANDROID.WMS, 2023/05/30, add end -->

    <!-- YaFeng.Geng@ANDROID.AMS, 2023/05/31, add for serviceDelay in Lowmemory -->
    <oplus-feature name="oplus.software.service.restrat_duration_low" />
    <!-- GaoWanpeng@ANDROID.POWER, 6464922, 2023/11/1, Add for keep screen_on during call when playing games	-->
    <oplus-feature name="oplus.software.game.keep_screen_on_during_call" />

    <!-- Fei.Luo@ANDROID.SENSOR, 2020/11/20, add for keyboard vibration convert -->
    <oplus-feature name="oplus.software.vibration_keyboard_convert" />
    <!-- Fei.Luo@ANDROID.SENSOR, 2020/11/20, add end -->

    <!-- Jiajie.Tan@Android.camera 2023/10/23, Add for double click volume button lunch camera -->
    <oplus-feature name="oplus.software.camera_volume_quick_launch" />
    <!-- Jiajie.Tan@Android.camera 2023/10/23, add end -->

    <!-- Fangwenjie@ANDROID.WMS, 2023/06/13, add for set low frame rate when panel expened -->
    <oplus-feature name="oplus.software.low_framerate_panel_expened" />
    <!-- Fangwenjie@ANDROID.WMS, 2023/06/13, add end -->
    
    <!-- GuiYuan.Tao@MULTIMEDIA.AUDIOSERVER.HAL.2024/07/11. Add for voice denoise -->
    <oplus-feature name="oplus.hardware.audio.voice_denoise_support"/>
    
</oplus-config>
